material Helmet_Greek
{
    receive_shadows on
    technique
    {
        pass
        {
            cull_hardware none

            ambient 0.8 0.8 0.8 1.0
            diffuse 0.64 0.64 0.64 1.0
            specular 0.16 0.16 0.16 1.0 12.5
            emissive 0.0 0.0 0.0 1.0

            texture_unit
            {
                texture helmet.jpg
                tex_address_mode wrap
                filtering trilinear
            }
        }
    }
}
