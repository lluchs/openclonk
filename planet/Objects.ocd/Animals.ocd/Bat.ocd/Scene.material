material Bat 
{
    receive_shadows on
    technique
    {
        pass
        {
        	cull_hardware none
			
            ambient 0.5 0.5 0.5 1.0
            diffuse 1.0 1.0 1.0 1.0

            texture_unit 
            {
                texture diffuse.png
                tex_address_mode wrap
                //tex_coord_set 0
                filtering trilinear
            }
        }
    }
}
